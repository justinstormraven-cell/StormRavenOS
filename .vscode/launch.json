{
    "configurations": [
        

        

        {
            "name": "Containers .NET Attach (Preview)",
            "platform": "netCore",
            "request": "attach",
            "sourceFileMap": {
                "/src": "${workspaceFolder}"
            },
            "type": "docker"
        },
        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/StormRaven.Shell/bin/Debug/net8.0-windows/StormRaven.Shell.exe",
            "args": [],
            "cwd": "${workspaceFolder}/StormRaven.Shell",
            "stopAtEntry": false,
            "console": "internalConsole"
        },
        {
            "name": "PowerShell: Run Pester Tests",
            "type": "PowerShell",
            "request": "launch",
            "script": "Invoke-Pester",
            "createTemporaryIntegratedConsole": true,
            "attachDotnetDebugger": true
        }
    ],
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0"
}